<Window x:Class="Flight_Sim.GraphWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Flight_Sim"
        mc:Ignorable="d"
        Title="GraphWindow" Height="450" Width="800">
    <Window.Resources>
        <Style x:Key="GridStyle1" TargetType="{x:Type Grid}"/>
    </Window.Resources>
    <Grid>
        <Grid HorizontalAlignment="Left" Height="357" Margin="25,31,0,0" VerticalAlignment="Top" Width="712" Style="{DynamicResource GridStyle1}"/>

    </Grid>
</Window>
