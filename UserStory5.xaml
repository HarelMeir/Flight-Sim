<UserControl x:Class="Flight_Sim.UserStory5"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Flight_Sim" d:DataContext="{d:DesignInstance Type=local:UserStory5VM}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <ListBox HorizontalAlignment="Left" Height="319" Margin="135,72,0,0" VerticalAlignment="Top" Width="191">
            <TextBox Name="Alitmeter" Height="30" TextWrapping="Wrap" Text="{Binding altimeterText}" Width="170"/>
            <TextBox Name="Airspeed" Height="30" TextWrapping="Wrap" Text="{Binding airspeedText}" Width="170"/>
        </ListBox>

    </Grid>
</UserControl>
